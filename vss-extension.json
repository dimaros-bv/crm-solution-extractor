{
    "manifestVersion": 1,
    "id": "d365-solution-extractor",
    "version": "0.1.0",
    "name": "D365 Solution Extractor",
    "publisher": "dimaros",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "icons":{
        "default": "images/dimaros-logo.png"
    },
    "files":[
        {
            "path": "solutionExtractor"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "scopes":[
        "vso.code_write"
    ],
    "contributions": [
        {
            "id": "solutionExtractor",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "solutionExtractor"
            }
        }
    ]
}
